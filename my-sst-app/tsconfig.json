{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "lib": ["es2017"],
      "strict": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "dist"
    },
    "include": ["stacks/**/*.ts", "src/**/*.ts"]
  }
  